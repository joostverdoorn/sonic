{
    "compilerOptions": {
        "target": "es6",
        "module": "es2015",
        "declaration": true,
        "noImplicitAny": true,
        "removeComments": true,
        "sourceMap": true,
        "outDir": "dist"
    },
    "filesGlob": [
        "src/**/*.ts"
    ],
    "files": [
        "src/async_iterator.ts",
        "src/bind.ts",
        "src/cache.ts",
        "src/entry.ts",
        "src/exceptions.ts",
        "src/factory.ts",
        "src/key.ts",
        "src/lens.ts",
        "src/observable.ts",
        "src/patch.ts",
        "src/promise_utils.ts",
        "src/range.ts",
        "src/sonic.ts",
        "src/state.ts",
        "src/store.ts",
        "src/tree.ts"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    }
}
